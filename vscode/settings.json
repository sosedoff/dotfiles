{
  "editor.fontSize": 15,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true
  },
  "editor.rulers": [
    80
  ],
  "editor.fontFamily": "SF Mono",
  "workbench.iconTheme": "vs-minimal",
  "editor.minimap.enabled": false,
  "workbench.colorCustomizations": {
    "activityBar.background": "#21252D",
    "sideBar.background": "#222830",
    "editor.background": "#21252D",
    "tab.inactiveBackground": "#121820",
    "titleBar.activeBackground": "#121820",
    "statusBar.background": "#121820"
  },
  "workbench.activityBar.visible": true,
  "editor.detectIndentation": false,
  "editor.tabSize": 2,
  "editor.autoIndent": true,
  "editor.showFoldingControls": "mouseover",
  "editor.wrappingIndent": "same",
  "editor.lineNumbers": "on",
  "editor.matchBrackets": true,
  "editor.mouseWheelScrollSensitivity": 0,
  "editor.trimAutoWhitespace": true,
  "editor.useTabStops": true,
  "editor.wordWrap": "off",
  "editor.formatOnPaste": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.snippetSuggestions": "top",
  "extensions.ignoreRecommendations": true,
  "workbench.colorTheme": "Base16 Ocean Dark",
  "workbench.startupEditor": "newUntitledFile",
  "editor.fontWeight": "600",
  "telemetry.enableTelemetry": false,
  "telemetry.enableCrashReporter": false,
  "go.useLanguageServer": true,
  "openapi.defaultPreviewRenderer": "redoc",
  "markdown-preview-enhanced.liveUpdate": false,
  "vs-kubernetes": {
    "vscode-kubernetes.helm-path.mac": "/Users/sosedoff/.vs-kubernetes/tools/helm/darwin-amd64/helm",
    "vscode-kubernetes.minikube-path.mac": "/Users/sosedoff/.vs-kubernetes/tools/minikube/darwin-amd64/minikube"
  },
  "files.insertFinalNewline": true,
  "redhat.telemetry.enabled": false
}
